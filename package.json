{
  "name": "torrent-report",
  "version": "1.0.0",
  "description": "torrent site stats",
  "main": "server/index.js",
  "scripts": {
    "dev-port": "lsof -i tcp:8080",
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "clear-logs": "rm -rf logs && mkdir logs",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:css": "stylelint './src/**/*.js'",
    "postinstall": "",
    "mock-scrape": "node server/reporterAgent/scrape/mock.js",
    "seed": "node script/seed.js",
    "seed-test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/TorrentReport-test' node script/seed-testDB.js",
    "start": "node server",
    "start-scrape": "NODE_ENV='development' node script/reporterAgent.js",
    "start-scrape-prod": "NODE_ENV='production' node script/reporterAgent.js",
    "start-dev": "yarn run build-client-watch & yarn run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "yarn run seed-test && NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/TorrentReport-test' mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --require babel-register",
    "test-server": "NODE_ENV='test' DATABASE_URL='postgres://root:root@localhost:5432/TorrentReport-test' mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --require babel-register",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "prepare": "npm run snyk-protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "",
  "license": "ISC",
